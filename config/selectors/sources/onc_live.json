{
    "OncLive": {
        "type": "catalog",
        "url": [
            "https://www.onclive.com/news"
        ],
        "selectors": {
            "source": "OncLive",
            "news": "//*[@class='w-full h-full']",
            "abstract": ".//p[@class=' mt-4 text-gray-800  pl-4']/text()",
            "published_date": ".//span[@class='text-sm text-gray-500 pl-4']/text()",
            "news_link": ".//p/a/@href"
        },
        "news_selector": {
            "title": "//*[@class='m-2 md:my-2 md:ml-2 md:pr-12']//h1/text()",
            "article_date": "//*[@class='m-2 md:my-2 md:ml-2 md:pr-12']//time/text()",
            "author": "//a[@class='text-author text-gray-500 hover:text-primary underline hover:no-underline decoration-gray-400']/text()",
            "content": "//*[@class='py-2']/div/p/text()"
        }
    }
}
